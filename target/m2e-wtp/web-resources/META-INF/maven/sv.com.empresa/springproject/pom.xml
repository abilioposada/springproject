	<modelVersion>4.0.0</modelVersion>
	<groupId>sv.com.empresa</groupId>
	<artifactId>springproject</artifactId>
	<packaging>war</packaging>
	<version>0.0.1-SNAPSHOT</version>
	<name>springproject Maven Webapp</name>
